naranja = #FC8608
gris = #C0BEBF
celeste = #5fceca
celes_osc = #4ca52a

.footer
	background-color celeste
	height 55px
	border-top 2px solid darken(celeste, 22%)
	
#icono_3
	width 30px
	
.div_icono_3
	display inline-block
	width 10%
	margin-top 3%
	
.div_text_muted
	display inline-block
	font-weight bold
	font-size 10px
	color white
	
.foot_text
	color white
	text-shadow 1px 1px 0px #727171
	
.center
	text-align center
	
/* Dispositivos peque√±os (tablets, anchura mayor o igual a 768px) */
@media (min-width: 600px)
	#icono_3
		width 32px
	
	.div_icono_3
		margin-top 2%
		
	.div_text_muted
		font-size 15px

/*@media (min-width: 768px)*/
 
/* Dispositivos medianos (ordenadores, anchura mayor o igual a 992px) */
@media (min-width: 992px)
	#icono_3
		width 35px
	
	.div_icono_3
		margin-top 1%
		
	.div_text_muted
		font-size 17px
 
/* Dispositivos grandes (ordenadores, anchura mayor o igual a 1200px) */
@media (min-width: 1200px)
	.div_icono_3
		margin-top 0.7%

	

	